{
    "manifest_version": 2,
    "name": "convenient new tab",
    "description": "This extension make your new tab convenient.",
    "version": "1.0",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "bookmarks"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
